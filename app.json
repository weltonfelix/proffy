{
  "expo": {
    "name": "Proffy",
    "slug": "proffy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8257E5"
    },
    "primaryColor": "#8257E5",
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#8257E5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "io.github.weltonfelix.proffy",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
